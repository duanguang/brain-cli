{
  "name": "brain-cli",
  "license": "MIT",
  "version": "0.3.18",
  "description": "A command line tool used to run  projects",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "rimraf dist && set NODE_ENV=dist && webpack --progress --colors -p",
    "serve:dist": "set NODE_ENV=dist && node server.js --env=dist",
    "start": "node bin/index.js ",
    "compile-dll": "webpack --config cfg/dll.js",
    "test": "node server.js",
    "brain-cli": "node bin/index.js",
    "dev": "node bin/index.js dev --smp=false --cssModules=false",
    "dll": "node bin/index.js dll",
    "build": "node bin/index.js build",
    "build:app3": "node bin/index.js build --apps=app1 --cssModules=false --smp=false ",
    "build:dist": "node bin/index.js build dist",
    "build:prod": "node bin/index.js build prod",
    "build:test": "node bin/index.js build test",
    "build:report": "node bin/index.js build -s",
    "bundle": "webpack bundleSize --profile --json --config='bin/index.js' --output-filename stats.json"
  },
  "homepage": "https://github.com/duanguang/brain-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duanguang/brain-cli"
  },
  "keywords": [
    "brain",
    "cli",
    "webpack-cli",
    "brain-cli",
    "react-cli",
    "react-webpack"
  ],
  "author": "duanguang She",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/commander": "2.3.31",
    "@types/invariant": "^2.2.28",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/object-hash": "^0.5.28",
    "@types/update-notifier": "^1.0.0",
    "@types/webpack": "^2.2.7",
    "chai": "^3.5.0",
    "copyfiles": "^1.0.0",
    "glob": "^7.1.1",
    "rimraf": "^2.4.3",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-detective": "^2.0.0",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-legion-hmr": "^0.0.15",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "commander": "^2.11.0",
    "core-js": "^2.0.0",
    "css-loader": "^0.28.0",
    "copy-webpack-plugin": "^4.0.1",
    "deep-assign": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "html-minifier": "^3.4.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "image-webpack-loader": "^3.2.0",
    "invariant": "^2.2.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "object-hash": "^1.1.5",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss-loader": "^1.3.2",
    "progress-bar-webpack-plugin": "^1.12.1",
    "raw-loader": "^0.5.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^2.0.2",
    "style-loader": "^0.18.2",
    "speed-measure-webpack-plugin":"^1.3.1",
    "ts-loader": "^2.3.7",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-config-standard": "^7.0.0",
    "tslint-config-alloy": "0.2.0",
    "update-notifier": "^2.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-spritesmith": "^1.0.0",
    "happypack": "^5.0.0",
    "typescript": "^3.4.5"
  },
  "main": "index.js",
  "bin": {
    "brain-cli": "./bin/index.js"
  }
}